<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

<div class="container emp-profile">
            <form method="post">
               {{#if profilePatient}}
                    <div class="row">
                        <div class="col-md-4">
                            <div class="profile-img">
                                <img src="{{profilePatient.ImageURL}}" alt=""/>
                                {{!-- <div class="file btn btn-lg btn-primary">
                                    Change Photo
                                    <input type="file" name="file"/>
                                </div> --}}
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="profile-head">
                                        <h5>
                                            {{profilePatient.Name}}
                                        </h5>
                                        <h6>
                                            {{profilePatient.Email}}
                                        </h6>
                                        
                                <ul class="nav nav-tabs" id="myTab" role="tablist">
                                    
                                    <li class="nav-item">

                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-2">
                        
                            <a href="/profile/{{profilePatient.Id}}/edit" type="submit" class="profile-edit-btn" name="btnAddMore" value="Edit Profile"/>
                                        Edit Profile
                            </a>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                        </div>
                        <div class="col-md-8">
                            <div class="tab-content profile-tab" id="myTabContent">
                                <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <label>User Id</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>
                                                {{profilePatient.Id}}
                                            </p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <label>Name</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>
                                                {{profilePatient.Name}}
                                            </p>
                                        </div>
                                        <div class="col-md-6">
                                            <label>Age:</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>
                                                {{profilePatient.Age}}
                                            </p>
                                        </div>
                                        <div class="col-md-6">
                                            <label>Sex</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>
                                                {{profilePatient.Sex}}
                                            </p>
                                        </div>
                                        <div class="col-md-6">
                                            <label>Address</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>
                                                {{profilePatient.Address}}
                                            </p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <label>Email</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>{{profilePatient.Email}}</p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <label>Phone</label>
                                        </div>
                                        <div class="col-md-6">
                                            <p>{{profilePatient.Phone}}</p>
                                        </div>
                                    </div>     
                                </div>
                            </div>
                        </div>
                    </div>
                {{else}}
                    <div class="row">
                        <div class="col-md-4">
                            <div class="profile-img">
                                <img src="{{profileDoctor.ImageURL}}" alt=""/>
                                {{!-- <div class="file btn btn-lg btn-primary">
                                    Change Photo
                                    <input type="file" name="file"/>
                                </div> --}}
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="profile-head">
                                        <h5>
                                            {{profileDoctor.Name}}
                                        </h5>
                                        <h6>
                                            {{profileDoctor.Email}}
                                        </h6>
                                        
                                <ul class="nav nav-tabs" id="myTab" role="tablist">
                                    
                                    <li class="nav-item">

                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-2">
                        
                            <a href="/profile/{{profileDoctor.Id}}/edit" type="submit" class="profile-edit-btn" name="btnAddMore" value="Edit Profile"/>
                                        Edit Profile
                            </a>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                        </div>
                        <div class="col-md-8">
                            <div class="tab-content profile-tab" id="myTabContent">
                                <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label>User Id</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>
                                                    {{profileDoctor.Id}}
                                                </p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label>Name</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>
                                                    {{profileDoctor.Name}}
                                                </p>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Age:</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>
                                                    {{profileDoctor.Age}}
                                                </p>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Sex</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>
                                                    {{profileDoctor.Sex}}
                                                </p>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Address</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>
                                                    {{profileDoctor.Address}}
                                                </p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label>Email</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>{{profileDoctor.Email}}</p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label>Phone</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>{{profileDoctor.Phone}}</p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label>Department</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>{{profileDoctor.Department}}</p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label> Description</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>{{profileDoctor.Description}}</p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label> Practicing certificate</label>
                                            </div>
                                            <div class="col-md-6">
                                                <p>{{profileDoctor.Practicing_certificate}}</p>
                                            </div>
                                        </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {{/if}}
            </form>           
        </div>